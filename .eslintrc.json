{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-console": "off",
        "no-plusplus": "off",
        "global-require": "off",
        "no-await-in-loop": "off",
        "no-param-reassign": "off",
        "no-underscore-dangle": "off",
        "no-restricted-syntax": "off"
    }
}
