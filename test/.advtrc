version: '1.0'

services:
  binary:
    name: geo
    dockerfile: .
    default_tag: latest
    # These define custom cli arguments, the left side is the --build-arg
    # and right the required --argument, all right side arguments will be
    # required and will fail if not supplied unless `--pass-args` is supplied
    build_arguments:
      - NPM_TAG=npm
    before_build:
      - echo before_build
    rm: true
